<h1> this is form submisssion for </h1>

 <%= form_for(@multi) do |f| %>

    <div class="field"> 
        <%= f.label :name %> 
        <%= f.check_box :name %> 
    </div>

       <%= f.radio_button :contactmethod, 'email', :checked => true %>
          <%= label :contactmethod_email, 'Email' %> 
       
       <%= f.radio_button :contactmethod, 'sms',:checked => true %> 
          <%= label :contactmethod_sms, 'SMS' %><br></br>

       <%=f.text_area :name, cols: "30", rows: "10" %><br></br>

        <%= f.label :password %>
          <%= f.password_field :password %><br></br>
        
        <%= f.label :confirm_password%>
          <%= f.password_field :password_confirmation %>



  

<% end %>
		